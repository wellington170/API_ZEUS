{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Orcamentos",
  "type": "object",

  "properties": {
    "nome": {
      "type": "string",
      "minLength":3
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "telefone": {
      "type": "string",
      "minLength":11,
      "maxLength": 14

    },
    "cpf_cnpj": {
      "type": "string",
      "minLength": 14,
      "maxLength": 18
    },
    "endereco": {
      "type": "string",
      "minLength": 5
    }
  },
    "required": ["nome",
    "email",
    "telefone",
    "cpf_cnpj"
  ]
}
