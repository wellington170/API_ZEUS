{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Membros",
  "type": "object",
  "properties": {
    "habilidades":{
      "type": "string",
      "minLength": 3
    },
    "nome_completo": {
      "type": "string",
      "minLength": 3
    },
    "email_institucional": {
      "type": "string",
      "format": "email"
    },
    "genero": {
      "type": "string",
      "enum": ["Masculino", "Feminino", "Outro"]
    },
    "telefone": {
      "type": "string",
      "minLength": 8,
      "maxLength": 15
    }
  }
}
