{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Membros",
  "type": "object",
  "properties": {
    "habilidades":{
      "type": "string",
      "minLength": 3
    },
    "nome_completo": {
      "type": "string",
      "minLength": 3
    },
    "data_de_ingresso": {
      "type": "string",
      "format": "date"
    },
    "email_institucional": {
      "type": "string",
      "format": "email"
    },
    "genero": {
      "type": "string",
      "enum": ["Masculino", "Feminino", "Outro"]
    },
    "cargo": {
      "type": "string",
      "enum": ["Desenvolvedor Backend", "Desenvolvedor Frontend", 
      "Designer", "Desenvolvedor Mobile",
      "Product Owner", "Analista de infraestrutura", 
      "Gestão de Pessoas", "Diretor de Projetos", 
      "Diretor de Marketing","Diretoria Comercial",
      "Acessor de Vendas", "Gerente de Vendas",
      "VPGG", "Diretor de Infraestrutura",
      "Diretor de Design","Marketing","Gestão de pessoas",
      "Presidência"]
    },
    "telefone": {
      "type": "string",
      "minLength": 11,
      "maxLength": 14
    },
    "administrador": {
      "type": "string",
      "enum": ["Sim", "Não"]
    }
  }
}
