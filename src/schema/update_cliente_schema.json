{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Orcamentos",
  "type": "object",

  "properties": {
    "nome": {
      "type": "string",
      "minLength":3
    },
    "email": {
      "type": "string",
      "format": "email"
    },
    "telefone": {
      "type": "string",
      "minLength":11,
      "maxLength": 14
    },
    "cep": {
      "type": "string",
      "minLength": 8,
      "maxLength": 8
    },
    "numero": {
      "type": "string",
      "minLength": 1,
      "maxLength": 5
    }
  }
}
