{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Membros",
  "type": "object",
  "properties": {
    "habilidades":{
      "type": "string"
    },
    "nome_completo": {
      "type": "string",
      "minLength": 3
    },
    "data_de_nascimento": {
      "type": "string",
      "format": "date"
    },
    "data_de_ingresso": {
      "type": "string",
      "format": "date"
    },
    "email_institucional": {
      "type": "string",
      "format": "email"
    },
    "genero": {
      "type": "string",
      "enum": ["Masculino", "Feminino", "Outro"]
    },
    "cargo": {
      "type": "string"
    },
    "telefone": {
      "type": "string",
      "minLength": 8,
      "maxLength": 15
    },
    "administrador": {
      "type": "string",
      "enum": ["true", "false"]
    }
  },
  "required": ["nome_completo",
  "data_de_ingresso",
  "data_de_nascimento",
  "email_institucional",
  "genero",
  "cargo",
  "telefone"]
}
